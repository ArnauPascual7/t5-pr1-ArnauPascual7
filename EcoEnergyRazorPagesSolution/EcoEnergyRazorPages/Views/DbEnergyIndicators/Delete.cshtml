@model EcoEnergyRazorPages.Model.DbEnergyIndicator

@{
    ViewData["Title"] = "Delete";
    Layout = "_Layout";
    @addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
}

<h1>Delete</h1>

<h3>Estas segur que vols eliminar això?</h3>
<div>
    <h4>Indicador d'energia</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Year)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Year)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CDEEBC_ProdNeta)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CDEEBC_ProdNeta)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CCAC_GasolinaAuto)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CCAC_GasolinaAuto)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CDEEBC_DemandaElectr)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CDEEBC_DemandaElectr)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CDEEBC_ProdDisp)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CDEEBC_ProdDisp)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-primary">Tornar</a>
    </form>
</div>
